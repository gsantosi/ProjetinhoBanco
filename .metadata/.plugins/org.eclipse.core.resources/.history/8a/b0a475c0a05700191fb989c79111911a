package Area;

public class Retangulo extends Shape {

	public double altura;
	public double largura;
	
	public Retangulo(Cor color, double altura, double largura) {
		super(color);
		
		this.altura = altura;
		this.largura = largura;
	}
	
	@Override
	public double Calculo(double valor) {
		double soma = 0.0;
		return soma;
	}

}
