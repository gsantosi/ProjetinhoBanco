package Files;

import java.io.File;
import java.util.Scanner;

public class BuscaNoArquivo {

	public static void main(String[] args) {
	
		
		File file = new File("C:\\Users\\gsantosi\\Desktop\\Guilherme (Indra)");
	    Scanner e = null;
	    
	    try {
	    	e = new Scanner(file);
	    	while(e.hasNextLine()) {
	    		System.out.println();
	    	}
	    }

	}

}
